{
  "name": "Simple Start Page",
  "description": "Replace new tab page with a simple start page",
  "version": "1.3",
  "author": "Josh Hawkins",
  "incognito": "split",
  "permissions": [
          "tabs",
          "storage"
        ],
  "chrome_url_overrides": {
    "newtab": "start.html"
  },
  "icons": {
      "16": "img/fav/favicon-16x16.png",
      "32": "img/fav/favicon-32x32.png",
      "96": "img/fav/favicon-96x96.png",
      "148": "img/fav/android-icon-48x48.png",
      "192": "img/fav/android-icon-192x192.png"
  },
  "content_security_policy": "script-src 'self' https://www.googleapis.com/; object-src 'self'",
  "manifest_version": 2
}
